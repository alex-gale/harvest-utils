{
  "name": "Harvest Utilities",
  "version": "1.0.0",
  "description": "Utilities to help with managing your day with Harvest",
  "manifest_version": 3,

  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["https://*.harvestapp.com/time/*"]
    }
  ],

  "web_accessible_resources": [{
    "resources": ["*/.js"],
    "matches": ["<all_urls>"]
  }]
}